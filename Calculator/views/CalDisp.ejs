<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
   <link rel='stylesheet' href='/stylesheets/style.css' />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
    <script src="http://code.jquery.com/ui/1.9.2/jquery-ui.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.2.26/angular.min.js"></script>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <script src="angularjs/calc.js"></script>
  <!-- <script type="text/javascript">
  var s = "";
 	function dispNum(n)
  {
	  var tbox=document.getElementById('exampleInputName2');
	  s=s+n;
	  tbox.value=s;
	  console.log(s);
  }
  </script> -->
  </head>
  <body ng-app="calc" ng-controller="myController">
    <div class="container">
    <form>
    <div class="row">
	 <div class="col-xs-12"></div>
	 <div class="col-xs-12"></div>
	  <div class="col-xs-12"></div>
    </div>
    
  	 <header class="row">
  	  <div class="col-xs-6 col-sm-5"></div>
  	  <div class="col-xs-6 col-sm-5 centered">
        	<h1 class="text-left">CALCULATOR</h1>
        </div>

    </header>
    
    <div class="row">
     <div class="col-sm-4"></div>
    <div class="col-sm-4">
   
      <input type="text" class="form-control" id="exampleInputName2" name="exampleInputName2" placeholder="Enter Expression" ng-model="exampleInputName2" /><br/>
      
    
      <div class="row"></div>
      <div class="row">
        <div class="col-sm-3 other">
    <button type="button" class="btn btn-default btn-lg" id="one" ng-click="append('1');">1</button>      
        </div>
        <div class="col-sm-3 other2">
         <button type="button" class="btn btn-default btn-lg" id="two" ng-click="append('2');">2</button>  
        </div>
        <div class="col-sm-3 other2">
          <button type="button" class="btn btn-default btn-lg" id="three" ng-click="append('3');">3</button>  
        </div>
        <div class="col-sm-3 other2">
          <button type="button" class="btn btn-default btn-lg" id="plus" ng-click="append('+');">+</button>  
        </div>
      </div>
      <div class="row">
        <div class="col-sm-3 other">
          <button type="button" class="btn btn-default btn-lg" id="for" ng-click="append('4');">4</button>
        </div>
        <div class="col-sm-3 other2">
         <button type="button" class="btn btn-default btn-lg" id="five" ng-click="append('5');">5</button>
        </div>
        <div class="col-sm-3 other2">
          <button type="button" class="btn btn-default btn-lg" id="six" ng-click="append('6');">6</button>
        </div>
        <div class="col-sm-3 other2">
          <button type="button" class="btn btn-default btn-lg" id="hyphen" ng-click="append('-');">-</button>
        </div>
      </div>
      <div class="row">
        <div class="col-sm-3 other">
           <button type="button" class="btn btn-default btn-lg" id="seven" ng-click="append('7');">7</button>
        </div>
        <div class="col-sm-3 other2">
           <button type="button" class="btn btn-default btn-lg id="eight" ng-click="append('8');">8</button>
        </div>
        <div class="col-sm-3 other2">
          <button type="button" class="btn btn-default btn-lg" id="nine" ng-click="append('9');">9</button>
        </div>
        <div class="col-sm-3 other2">
          <button type="button" class="btn btn-default btn-lg" id="multiply" ng-click="append('*');">*</button>
        </div>
      </div>
      <div class="row">
        <div class="col-sm-3 other">
           <button type="button" class="btn btn-default btn-lg" id="dot" ng-click="append('.');">.</button>
        </div>
        <div class="col-sm-3 other2">
           <button type="button" class="btn btn-default btn-lg" id="zero" ng-click="append('0');">0</button>
        </div>
        <div class="col-sm-3 other2">
           <button type="button" class="btn btn-default btn-lg" id="divide" ng-click="append('/');">/</button>
        </div>
        
        <div class="col-sm-3 other2">
         <!--   <button type="submit" class="btn btn-default btn-lg" id="equals" ng-click="append('1');">=</button> -->
         
           <button type="button" class="btn btn-default btn-lg" id="equals" ng-click="submit();">=</button>
        
        </div>
       
      </div>
    </div>
    
     
 </form>
</div>
  </body>
</html>